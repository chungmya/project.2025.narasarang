{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "javascriptreact"],
  "css.validate": false,
  "scss.validate": false,
  "emmet.includeLanguages": {
    "scss": "css"
  },
  "files.associations": {
    "*.scss": "scss"
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/*.map": true,
    "**/src/assets/css": true
  },
  "files.eol": "auto"
}
